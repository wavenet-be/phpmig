{
    "name": "wavenet-be/phpmig",
    "type": "library",
    "description": "Simple migrations system for php",
    "keywords": ["migrations", "database migrations"],
    "homepage": "https://github.com/wavenet-be/phpmig",
    "license": "MIT",
    "authors": [{
        "name": "Dave Marshall",
        "homepage": "http://davedevelopment.co.uk"
    },
    {
        "name": "David Nielsen"
    },
    {
        "name": "Sven Keil",
        "homepage": "https://skeilit.de"
    }],
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/console": "^6.0||^7.0",
        "symfony/config": "^4.0||^5.0||^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.5",
        "mockery/mockery": "*@dev"
    },
    "suggest": {
        "pimple/pimple": "Pimple allows to bootstrap phpmig really easily.",
        "mongodb/mongodb": "This library is required for the MongoDb adapter"
    },
    "autoload": {
        "psr-0": {
            "Phpmig": "src/"
        }
    },
    "bin": ["bin/phpmig"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.7.x-dev"
        }
    }
}
